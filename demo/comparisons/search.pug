include ../mixins.pug

.comparison
  .render
    +androidstatusbar('rgb(var(--primary-700-color))')
    .mdw-toolbar.mdw-theme__filled(mdw-color='primary' mdw-tone='500')
      .mdw-toolbar__action
        .mdw-toolbar__start
          button.mdw-button(mdw-icon mdw-active).material-icons arrow_back
        .mdw-toolbar__title.mdw-textfield#search-textfield(mdw-solo mdw-color="accent")
          input.mdw-textfield__input(placeholder="Search")
        .mdw-toolbar__end
          button.mdw-button(mdw-icon mdw-active).material-icons clear
    .content.mdw-theme__filled(mdw-color="background" mdw-tone="50")
      ul.mdw-list#search-list
        li.mdw-list__row
          .mdw-list__start
            i.material-icons access_time
