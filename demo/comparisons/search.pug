include ../mixins.pug

.comparison
  .render
    +androidstatusbar('rgb(var(--primary-700-color))')
    .mdw-toolbar.mdw-theme__filled(mdw-color='primary' mdw-tone='500')
      .mdw-toolbar__action
        .mdw-toolbar__start
          button.mdw-button(mdw-icon mdw-active).material-icons arrow_back
        .mdw-toolbar__title.mdw-textfield#search-textfield-simple(mdw-solo mdw-color="accent")
          input.mdw-textfield__input(placeholder="Search")
        .mdw-toolbar__end
          button.mdw-button(mdw-icon mdw-active).material-icons clear
    .content.mdw-theme__filled(mdw-color="background" mdw-tone="50")
      ul.mdw-list#search-list-simple
        li.mdw-list__row Attractions
        li.mdw-list__row Dining
        li.mdw-list__row Education
        li.mdw-list__row Family
        li.mdw-list__row Health
        li.mdw-list__row Office
        li.mdw-list__row Promotions
        li.mdw-list__row Radio
        li.mdw-list__row Recipes
        li.mdw-list__row Sports
        li.mdw-list__row Travel
    +androidnavbar
  +target('https://storage.googleapis.com/material-design/publish/material_v_12/assets/0B5ZSepuCX1xObnc4ZFYybVRWY2s/single-action4.png')

